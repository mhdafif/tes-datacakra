{
  "name": "csr-react-fe-boilerplate",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "predev": "if [ $(node -v) != \"$(cat .nvmrc)\" ]; then echo \"Error: Node.js version does not match the required version. Please use Node.js $(cat .nvmrc).\"; exit 1; fi",
    "start": "vite",
    "dev": "pnpm predev && vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "prettier:check": "prettier . --ignore-path .prettierignore --check",
    "prettier:fix": "prettier . --ignore-path .prettierignore --write",
    "lint": "eslint .",
    "lint:fix": "eslint --fix-dry-run --ext .tsx,.ts .",
    "type:check": "tsc",
    "validate": "pnpm lint && pnpm type:check && pnpm prettier:check",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "build-tailwind": "tailwindcss -i ./src/styles/_main.css -o ./src/styles/tailwind.css --minify"
  },
  "dependencies": {
    "@million/lint": "1.0.14",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-visually-hidden": "^1.1.2",
    "@tanstack/react-query": "^5.67.1",
    "axios": "^1.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "lucide-react": "^0.477.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^7.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vite-plugin-image-optimizer": "^1.1.8",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.21.0",
    "eslint-config-love": "^119.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.16.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  }
}
